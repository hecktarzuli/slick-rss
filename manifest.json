{
  "name": "Slick RSS",
  "version": "2.993",
  "manifest_version": 2,
  "description": "A full featured RSS reader that's fully contained within the browser.",
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "bookmarks"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "Slick RSS"
  }
}